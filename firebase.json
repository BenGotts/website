{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "function": "index"
      },
      {
        "source": "/comps",
        "function": "comps"
      },
      {
        "source": "/pblTrainer",
        "function": "pblTrainer"
      }
    ]
  }
}
